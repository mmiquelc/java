package Tasca7_EstructuresRepeticio2;

import java.util.Iterator;

public class Repeticio2_4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		final int x = 4;
		final int y = 4;
		
		for (int i = 0; i < x; i++ ) {
			for (int j = 0; j < y; j++) {
				System.out.println("* * * *");
			}
		}
	}

}
