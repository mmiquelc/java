package matrius;

public class MatriuBidimensional {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int [] vector1 = {1, 2, 3};
		int [] vector2 = {10, 20, 30};
		int [][] matriu = new int[2][];]		matriu[0] = vector1;
		matriu[]
		
		
		for (int i = 0; i<2; i++) {
			for(int j = 0; j<3; j++) {
				System.out.print(matriu[i][j] + " ");
			}
			System.out.println();
		}
		
		
	}

